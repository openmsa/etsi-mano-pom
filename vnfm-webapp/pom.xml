			<version>${version.event-jms}</version>
		</dependency>
		<dependency>
			<groupId>org.hibernate.validator</groupId>
			<artifactId>hibernate-validator</artifactId>
		</dependency>
		<dependency>
			<groupId>com.ubiqube.mano</groupId>
			<artifactId>mano-vim-k8s</artifactId>
			<version>${version.mano-vim-k8s}</version>
		</dependency>
		<dependency>
			<groupId>com.ubiqube.mano.pkg</groupId>
			<artifactId>k8s-parser</artifactId>
			<version>${version.k8s-parser}</version>
		</dependency>
		<!--
		<dependency>
			<groupId>com.ubiqube.mano.service.mon</groupId>
